{
  "name": "zleslie-pxe",
  "version": "1.0.0",
  "author": "zleslie",
  "summary": "A Puppet module to deploy and manage a Pre eXecution Environment",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-pxe.git",
  "project_page": "https://github.com/xaque208/puppet-pxe",
  "issues_url": "https://github.com/xaque208/puppet-pxe/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10",
        "9.3",
        "8.4"
      ]
    }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 3.8.0 < 5.0.0" }
  ],
  "dependencies": [
    {"name":"puppetlabs/concat","version_requirement":">=1.2.1 < 2.3.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">=4.4.0 < 4.16.0"},
    { "name": "puppetlabs/tftp", "version_requirement": ">= 0.2.0 < 1.0.0" }
  ]
}
